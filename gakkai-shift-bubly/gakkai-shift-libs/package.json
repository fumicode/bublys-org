{
  "name": "@bublys-org/gakkai-shift-libs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "dev": "tsc -p tsconfig.lib.json --watch",
    "build:bubly": "vite build -c vite.config.bubly.ts && cp dist-bubly/bubly.js ../gakkai-shift-app/public/bubly.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@bublys-org/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@bublys-org/gakkai-shift-model": "*",
    "@bublys-org/bubbles-ui": "*",
    "@bublys-org/state-management": "*"
  },
  "nx": {
    "targets": {
      "dev": { "continuous": true }
    }
  }
}
