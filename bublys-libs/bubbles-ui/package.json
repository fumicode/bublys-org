{
  "name": "@bublys-org/bubbles-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "dev": "tsc -p tsconfig.lib.json --watch"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@bublys-org/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./debug": {
      "@bublys-org/source": "./src/debug.ts",
      "types": "./dist/debug.d.ts",
      "import": "./dist/debug.js",
      "default": "./dist/debug.js"
    }
  },
  "dependencies": {
    "@bublys-org/bubbles-ui-util": "*",
    "@bublys-org/state-management": "*",
    "@mui/icons-material": "^7.0.0",
    "@reduxjs/toolkit": "^2.9.0",
    "clsx": "^2.1.0",
    "immer": "^10.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "styled-components": "^5.3.6"
  },
  "nx": {
    "targets": {
      "dev": { "continuous": true }
    }
  }
}
